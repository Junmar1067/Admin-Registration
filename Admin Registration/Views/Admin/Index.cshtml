@model IEnumerable<Admin_Registration.Models.Account>


@Html.ActionLink("Add", "Add")

<form method="get" action="@Url.Action("Search", "Admin")">
    <label>Search</label>
    <input type="text" name="search" />
    <button type="submit">Search</button>
</form>


<br/>                                       
    
<table style="width:70%">
  <tr>
    <td><b>ID</b></td>
    <td><b>Name</b></td>
    <td><b>Age</b></td> 
    <td><b>Address</b></td>
    <td></td>
    <td></td>
  </tr>
@foreach (var item in Model)
{ 
    <tr>
    <td>@Html.DisplayFor(sdfg => item.Id)</td>

    <td>@Html.DisplayFor(sdfg => item.Name)</td>
    
    <td>@Html.DisplayFor(sdfg => item.Age)</td>
    
    <td>@Html.DisplayFor(sdfg => item.Address)</td>
    
    <td>@Html.ActionLink("Edit", "Edit", new { Id = item.Id })</td>
    
    <td> @Html.ActionLink("Delete", "Delete", new { Id = item.Id })</td>
    </tr>
}
</table>